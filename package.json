{
  "name": "bestofjs-webui",
  "version": "0.35.0",
  "description": "Best of JS front-end web application",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "cross-env VITE_APP_VERSION=$npm_package_version vite",
    "start:mock": "cross-env VITE_APP_MOCK=1 npm run start",
    "build": "cross-env VITE_APP_VERSION=$npm_package_version vite build",
    "preview": "vite preview",
    "test": "node scripts/test.js --watch",
    "test:ci": "node scripts/test.js --coverage --watchAll=false --runInBand --forceExit",
    "jest-preview": "jest-preview",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "sort": "npx sort-package-json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bestofjs/bestofjs-webui"
  },
  "keywords": [
    "react",
    "vite"
  ],
  "author": "Michael Rambeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bestofjs/bestofjs-webui/issues"
  },
  "homepage": "https://bestofjs.org",
  "dependencies": {
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@headlessui/react": "^1.4.0",
    "@vitejs/plugin-react": "^2.0.0",
    "cross-env": "^7.0.3",
    "dompurify": "^2.3.8",
    "framer-motion": "^4.1.17",
    "numeral": "^1.5.3",
    "qs": "^6.9.0",
    "react": "^17.0.2",
    "react-content-loader": "^5.1.2",
    "react-dom": "^17.0.2",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^4.3.1",
    "react-simple-animate": "^3.3.6",
    "react-toggled": "^1.0.2",
    "react-use": "^15.3.0",
    "react-vertical-timeline-component": "^3.0.2",
    "reselect": "^4.0.0",
    "serve": "^13.0.2",
    "swr": "^0.2.3",
    "typescript": "^4.6.3",
    "unfetch": "^4.1.0",
    "unstated-next": "^1.1.0",
    "use-debounce": "^3.0.0",
    "vague-time": "^2.4.1",
    "vite": "^3.0.2",
    "vite-plugin-checker": "^0.4.9",
    "vite-plugin-html": "^3.2.0",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.21",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/debug": "^4.1.5",
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-testing-library": "^5.5.0",
    "husky": "^0.14.3",
    "jest": "^27.5.1",
    "jest-preview": "^0.2.3",
    "jest-watch-typeahead": "^1.1.0",
    "msw": "^0.41.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^1.4.1",
    "whatwg-fetch": "^3.6.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "msw": {
    "workerDirectory": "public"
  }
}
