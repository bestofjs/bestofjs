{
  "name": "bestofjs-webui",
  "version": "0.27.4",
  "description": "Best of JS front-end web application",
  "engines": {
    "node": ">=12.16.1"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build && node ./build-meta",
    "test": "react-scripts test --env=jsdom",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bestofjs/bestofjs-webui"
  },
  "keywords": [
    "react",
    "create-react-app"
  ],
  "author": "Michael Rambeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bestofjs/bestofjs-webui/issues"
  },
  "homepage": "https://bestofjs.org",
  "dependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "cross-env": "^1.0.7",
    "lodash": "^4.17.19",
    "lodash.times": "^4.3.2",
    "numeral": "^1.5.3",
    "qs": "^6.9.0",
    "react": "16.13.1",
    "react-calendar-heatmap": "1.4.1",
    "react-content-loader": "^5.1.2",
    "react-dom": "16.13.1",
    "react-icons": "^3.10.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.0",
    "react-select": "^3.0.4",
    "react-simple-animate": "^3.3.6",
