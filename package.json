    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^0.14.3",
    "playwright": "1.2.0",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "qawolf": "1.2.0",
    "react-testing-library": "^4.1.2",
    "ts-jest": "^26.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-unused-vars": "error"
    }
  }
