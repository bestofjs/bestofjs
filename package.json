{
  "name": "bestofjs-webui",
  "version": "0.34.0",
  "description": "Best of JS front-end web application",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "cross-env VITE_APP_VERSION=$npm_package_version vite",
    "build:only": "cross-env VITE_APP_VERSION=$npm_package_version vite build && node ./build-meta",
    "build": "cross-env VITE_APP_VERSION=$npm_package_version tsc && vite build && node ./build-meta",
    "preview": "vite preview",
    "test": "react-scripts test --env=jsdom",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "sort": "npx sort-package-json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bestofjs/bestofjs-webui"
  },
  "keywords": [
    "react",
    "create-react-app"
  ],
  "author": "Michael Rambeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bestofjs/bestofjs-webui/issues"
  },
  "homepage": "https://bestofjs.org",
  "dependencies": {
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@headlessui/react": "^1.4.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cross-env": "^7.0.3",
    "framer-motion": "^4.1.17",
    "fs-extra": "^10.1.0",
    "numeral": "^1.5.3",
    "qs": "^6.9.0",
    "react": "^17.0.2",
    "react-content-loader": "^5.1.2",
    "react-dom": "^17.0.2",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^4.3.1",
    "react-simple-animate": "^3.3.6",
    "react-toggled": "^1.0.2",
    "react-use": "^15.3.0",
    "react-vertical-timeline-component": "^3.0.2",
    "reselect": "^4.0.0",
    "serve": "^13.0.2",
    "swr": "^0.2.3",
    "tinytime": "^0.2.6",
    "typescript": "^4.6.3",
    "unfetch": "^4.1.0",
    "unstated-next": "^1.1.0",
    "use-debounce": "^3.0.0",
    "vague-time": "^2.4.1",
    "vite": "^2.9.9",
    "vite-plugin-checker": "^0.4.6",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^0.14.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^1.4.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  }
}
